<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="m2y.centennial.healthowl">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".menu" />
        <activity
            android:name=".Patient.patientList"
            android:parentActivityName=".menu">
            <meta-data
                android:name="android.support.ParentACTIVITY"
                android:value=".menu" />
        </activity>
        <activity android:name=".Patient.patientMain" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".appointment.MainAppointments">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".appointment.AppointmentTabs" />
        <activity android:name=".MainGroup" />
        <activity android:name=".test.TestMainActivity" />
        <activity android:name=".test.TestDetails" />
        <activity android:name=".test.TestAdd" />
        <activity android:name=".groups.GroupMainActivity" />
        <activity android:name=".groups.GroupAdd" />
        <activity android:name=".groups.GroupDetails"></activity>
    </application>

</manifest>
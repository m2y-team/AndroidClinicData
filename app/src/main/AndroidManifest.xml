<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="m2y.centennial.healthowl">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".menu"></activity>
        <activity
            android:name=".patient.patientList"
            android:parentActivityName=".menu">
            <meta-data
                android:name="android.support.ParentACTIVITY"
                android:value=".menu" />
        </activity>
        <activity android:name=".patient.patientMain"></activity>
        <activity android:name=".patient.PatientFragmentAdapter"></activity>
        <activity android:name=".Login">

        </activity>
        <activity android:name=".appointment.MainAppointments">
            android:parentActivityName=".menu">
            <meta-data
                android:name="android.support.ParentACTIVITY"
                android:value=".menu" />
        </activity>
        <activity android:name=".appointment.AppointmentTabs">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".appointment.addAppointment"></activity>
    </application>

</manifest>